@LAUNCH_HEADER@
{
    "version": "0.2.0",

    "configurations": [     
        {
            "name": "Launch Remote jlink",
            "request": "launch",
            "type": "cortex-debug",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/bmplc.elf",
            "svdFile": "${workspaceFolder}/@DEVICE_SVD_FILE@",
            "servertype": "jlink",
            "ipAddress": "${input:IPAddressID}",
            "interface": "swd",
            "device": "@DEVICE_TYPE@",
            "gdbPath": "arm-none-eabi-gdb",
            // "preLaunchTask": "CMake: build",
            "runToMain": true,
            "runToEntryPoint": "main",
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "",
                        "port": 0,
                        "type": "console"
                    }
                ]
            },
            "rtos": "FreeRTOS",
            "showDevDebugOutput": "none"
        },
        {
            "name": "Attach Remote jlink",
